{
  "server": "hub-cloud.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "build": "BROWSERSTACK_BUILD_NAME",
    "project": "BrowserStack demo - Web",
    "name": "parallel_test",
    "browserstack.debug": true,
    "browserstack.networkLogs": true,
    "browserstack.console": "verbose"
  },

  "environments": {
    "chrome": {
      "browser": "chrome",
      "browser_version": "latest-1"
    },
    "firefox": {
      "browser": "firefox",
      "browser_version": "latest-2"
    },
    "safari": {
      "os": "OS X",
      "os_version": "Big Sur",
      "browser": "Safari",
      "browser_version": "14.0"
    },
    "android": {
      "os": "Windows",
      "os_version": "10",
      "browser": "Edge",
      "browser_version": "latest-1"
    },
    "iphone":{
      "os": "OS X",
      "os_version": "Catalina",
      "browser": "Chrome",
      "browser_version": "latest-2"
    }
  }
}
